#!/usr/bin/env php
<?php

namespace byrokrat\accounting\Sie4\Parser;

require __DIR__ . '/../vendor/autoload.php';

$content = file_get_contents($argv[1]);

try {
    (new ParserFactory)->createParser(ParserFactory::FAIL_ON_NOTICE)->parse($content);
} catch (\Exception $e) {
    echo $e->getMessage();
}

echo "\n\nDONE! \n";
